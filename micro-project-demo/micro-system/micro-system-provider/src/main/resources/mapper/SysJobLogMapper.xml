<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atqgh.mapper.SysJobLogMapper">

	<!-- 结果集映射 -->
    <resultMap id="sysJobLogMap" type="com.atqgh.dto.SysJobLogDto" >
        <result property="id" column="id"/>
        <result property="jobName" column="job_name"/>
        <result property="jobGroup" column="job_group"/>
        <result property="invokeTarget" column="invoke_target"/>
        <result property="jobMessage" column="job_message"/>
        <result property="status" column="status"/>
        <result property="exceptionInfo" column="exception_info"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

    <!-- 结果集映射 -->
    <resultMap id="sysJobLogDetailMap" type="com.atqgh.dto.SysJobLogDto" >
        <result property="id" column="id"/>
        <result property="jobName" column="job_name"/>
        <result property="jobGroup" column="job_group"/>
        <result property="invokeTarget" column="invoke_target"/>
        <result property="jobMessage" column="job_message"/>
        <result property="status" column="status"/>
        <result property="exceptionInfo" column="exception_info"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

    <!-- 字段分片 -->
    <sql id="Base_Column_List">
        id,
        job_name,
        job_group,
        invoke_target,
        job_message,
        status,
        exception_info,
        create_time
    </sql>

    <!-- 字段分片 -->
    <sql id="Base_Column_Detail">
        id,
        job_name,
        job_group,
        invoke_target,
        job_message,
        status,
        exception_info,
        create_time
    </sql>

    <!-- 分页字段分片 -->
    <sql id="Base_Column_Page">
        id AS id,
        job_name AS jobName,
        job_group AS jobGroup,
        invoke_target AS invokeTarget,
        job_message AS jobMessage,
        status AS status,
        exception_info AS exceptionInfo,
        create_time AS createTime
    </sql>

</mapper>